    "./.claude/commands/laravel/command-new.md",
    "./.claude/commands/laravel/job-new.md",
    "./.claude/commands/laravel/event-new.md",
    "./.claude/commands/laravel/middleware-new.md",
    "./.claude/commands/laravel/policy-new.md",
    "./.claude/commands/laravel/test-new.md",
    "./.claude/commands/livewire/component-new.md",
